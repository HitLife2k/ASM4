<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asm4">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ASM4">
        <activity android:name=".url_image" android:screenOrientation="portrait"></activity>// su dung android:screenOrientation="portrait" de ngan khong cho dien thoai xoay ngang
        <activity android:name=".MainActivity"  android:screenOrientation="portrait">// su dung android:screenOrientation="portrait" de ngan khong cho dien thoai xoay ngang
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>